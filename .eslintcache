rebaseapp.com',\n  projectId: 'netflix-961e6',\n  storageBucket: 'netflix-961e6.appspot.com',\n  messagingSenderId: '686056932198',\n  appId: '1:686056932198:web:99ec4c7a9b1f98315d3ea7',\n};\n\nconst firebase = Firebase.initializeApp(config);\n\n//seedDatabase(firebase); ====> careful here - if uncommented and reload the app it will upload the same data to the firestore which will duplicate the data data\n\nexport { firebase };\n","/home/oleg/Desktop/myProjects/netflix-clone/src/seed.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/context/firebase.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/components/form/index.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/components/form/styles/form.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/helpers/routes.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/hooks/index.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/hooks/use-auth-listener.js",["226"],"import { useState, useEffect, useContext } from 'react';\nimport { FirebaseContext } from '../context/firebase';\n\nexport default function useAuthListener() {\n  const [user, setUser] = useState(\n    JSON.parse(localStorage.getItem('authUser'))\n  );\n  const { firebase } = useContext(FirebaseContext);\n\n  useEffect(() => {\n    const listener = firebase.auth().onAuthStateChanged((authUser) => {\n      if (authUser) {\n        localStorage.setItem('authUser', JSON.stringify(authUser));\n        setUser(authUser);\n      } else {\n        localStorage.removeItem('authUser');\n        setUser(null);\n      }\n    });\n\n    return () => listener(); // clean up the listener\n  }, []);\n\n  return { user };\n}\n","/home/oleg/Desktop/myProjects/netflix-clone/src/hooks/use-content.js",["227"],"import { useEffect, useState, useContext } from 'react';\nimport { FirebaseContext } from '../context/firebase';\n\nexport default function useContent(target) {\n  const [content, setContent] = useState([]);\n  const { firebase } = useContext(FirebaseContext);\n\n  useEffect(() => {\n    firebase\n      .firestore()\n      .collection(target)\n      .get()\n      .then((snapshot) => {\n        const allContent = snapshot.docs.map((contentObj) => ({\n          ...contentObj.data(),\n          docId: contentObj.id,\n        }));\n\n        setContent(allContent);\n      })\n      .catch((error) => {\n        console.log(error.message);\n      });\n  }, []);\n\n  return { [target]: content };\n}\n","/home/oleg/Desktop/myProjects/netflix-clone/src/utils/selection-filter.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/containers/browse.js",["228","229"],"import React, { useState, useEffect, useContext } from 'react';\nimport Fuse from 'fuse.js';\nimport logo from '../Netflix_logo.svg';\nimport * as ROUTES from '../constants/routes';\nimport {\n  Loading,\n  Header,\n  Billboard,\n  Main,\n  Player,\n  CategoryCarousel,\n} from '../components';\nimport { FirebaseContext } from '../context/firebase';\nimport { SelectProfileContainer } from './profiles';\nimport { FooterContainer } from './footer';\n\nexport function BrowseContainer({ slides }) {\n  const [category, setCategory] = useState('series');\n  const [profile, setProfile] = useState({});\n  const [loading, setLoading] = useState(true);\n  const [searchTerm, setSearchTerm] = useState('');\n  const [slideRows, setSlideRows] = useState([]);\n  const [showMenu, setShowMenu] = useState(false);\n\n  const { firebase } = useContext(FirebaseContext);\n  const user = firebase.auth().currentUser || {};\n\n  useEffect(() => {\n    setTimeout(() => {\n      setLoading(false);\n    }, 3000);\n  }, [profile.displayName]);\n\n  useEffect(() => {\n    setSlideRows(slides[category]);\n  }, [slides, category]);\n\n  useEffect(() => {\n    const fuse = new Fuse(slideRows, {\n      keys: ['data.description', 'data.title', 'data.genre'],\n    });\n\n    const results = fuse.search(searchTerm).map(({ item }) => item);\n\n    if (slideRows.length > 0 && searchTerm.length > 3 && results.length > 0) {\n      setSlideRows(results);\n    } else {\n      setSlideRows(slides[category]);\n    }\n  }, [searchTerm]);\n\n  const handleClick = () => {\n    setShowMenu(!showMenu);\n  };\n\n  return profile.displayName ? (\n    <div style={{ background: '#141414' }}>\n      {loading ? <Loading src={user.photoURL} /> : <Loading.ReleaseBody />}\n\n      {/* ====================\n          HEADER START \n      =====================*/}\n      <Header bgr={false}>\n        <Header.Sticky justify={'flex-start'}>\n          <Header.Logo to={ROUTES.HOME} src={logo} alt='Netflix' />\n          <Header.OpenMenu openMenu onClick={() => handleClick()}>\n            <Header.Link>Browse</Header.Link>\n          </Header.OpenMenu>\n          <Header.PrimaryNav showMenu={showMenu}>\n            <Header.NavTab>\n              <Header.Link\n                active={category === 'series' ? 'true' : 'false'}\n                onClick={() => setCategory('series')}\n              >\n                Home\n              </Header.Link>\n            </Header.NavTab>\n            <Header.NavTab>\n              <Header.Link\n                active={category === 'series' ? 'true' : 'false'}\n                onClick={() => setCategory('series')}\n              >\n                Series\n              </Header.Link>\n            </Header.NavTab>\n            <Header.NavTab>\n              <Header.Link\n                active={category === 'films' ? 'true' : 'false'}\n                onClick={() => setCategory('films')}\n              >\n                Films\n              </Header.Link>\n            </Header.NavTab>\n            <Header.NavTab>\n              <Header.Link to={ROUTES.HOME}>New & Popular</Header.Link>\n            </Header.NavTab>\n            <Header.NavTab>\n              <Header.Link to={ROUTES.HOME}>My List</Header.Link>\n            </Header.NavTab>\n          </Header.PrimaryNav>\n          <Header.SecondaryNav>\n            <Header.Search\n              searchTerm={searchTerm}\n              setSearchTerm={setSearchTerm}\n            />\n            <Header.Gifts></Header.Gifts>\n            <Header.Notifications>\n              <Header.Dropdown>\n                <Header.Group>\n                  <p>Hello</p>\n                </Header.Group>\n              </Header.Dropdown>\n            </Header.Notifications>\n            <Header.Profile>\n              <Header.ProfileImg src={user.photoURL} />\n              <Header.Dropdown>\n                <Header.Group>\n                  <Header.Link>\n                    <Header.ProfileImg src={user.photoURL} />\n                    {user.displayName}\n                  </Header.Link>\n                  <Header.Link>Manage Profiles</Header.Link>\n                </Header.Group>\n                <Header.Group>\n                  <Header.Link>Account</Header.Link>\n                  <Header.Link>Help Centre</Header.Link>\n                  <Header.Link onClick={() => firebase.auth().signOut()}>\n                    Sign out of Netflix\n                  </Header.Link>\n                </Header.Group>\n              </Header.Dropdown>\n            </Header.Profile>\n          </Header.SecondaryNav>\n        </Header.Sticky>\n      </Header>\n\n      {/* ====================\n          BILLBOARD START \n      =====================*/}\n      <Billboard>\n        <Billboard.Row>\n          <Billboard.Trailer>\n            <Billboard.Poster\n              src={'/images/misc/billboard/how-to-get-away-with-murder.webp'}\n            />\n          </Billboard.Trailer>\n          <Billboard.Info>\n            <Billboard.Logo>\n              <Billboard.LogoImg\n                src={\n                  '/images/misc/billboard/logo-how-to-get-away-with-murder.webp'\n                }\n              />\n            </Billboard.Logo>\n            <Billboard.Msg>Watch Season 6 now</Billboard.Msg>\n            <Billboard.Description>\n              A law professor gives her students a shot at some real-world\n              cases. Too bad that includes embroiling them in murder.\n            </Billboard.Description>\n            <Billboard.Links>\n              <Billboard.PlayBtn>Play</Billboard.PlayBtn>\n              <Billboard.MoreInfoBtn>More Info</Billboard.MoreInfoBtn>\n            </Billboard.Links>\n          </Billboard.Info>\n        </Billboard.Row>\n      </Billboard>\n\n      {/* ====================\n          MAIN CONTENT START \n      =====================*/}\n      <Main.MainView>\n        {slideRows.map((slideItem) => (\n          <Main key={`${category}-${slideItem.title.toLowerCase()}`}>\n            <Main.Title>{slideItem.title}</Main.Title>\n            <CategoryCarousel category={category} data={slideItem.data} />\n          </Main>\n        ))}\n      </Main.MainView>\n      <FooterContainer />\n    </div>\n  ) : (\n    <SelectProfileContainer user={user} setProfile={setProfile} />\n  );\n}\n","/home/oleg/Desktop/myProjects/netflix-clone/src/containers/profiles.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/components/profiles/index.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/components/profiles/styles/profiles.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/components/loading/index.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/components/loading/styles/loading.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/components/card/index.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/components/card/styles/card.js",[],"/home/oleg/Desktop/myProjects/netflix-clone/src/components/player/index.js",["230"],"import React, { useState, useContext, createContext } from 'react';\nimport ReactDOM from 'react-dom';\nimport { Container, Button, Overlay, Inner, Close } from './styles/player';\n\nexport const PlayerContext = createContext();\n\nexport default function Player({ children, ...restProps }) {\n  const [showPlayer, setShowPlayer] = useState(false);\n\n  return (\n    <PlayerContext